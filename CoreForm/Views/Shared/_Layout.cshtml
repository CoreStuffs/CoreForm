<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CoreForm</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <!-- Google Fonts Roboto -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">

    <link rel="stylesheet" href="@Url.Content("~/lib/uikit/css/uikit.min.css")">
    @RenderSection("StyleSheets", required: false)
</head>
<body class="uk-text-normal">
    <div uk-sticky="media: 960" class="uk-navbar-container tm-navbar-container uk-sticky uk-active uk-sticky-below uk-sticky-fixed" style="position: fixed; top: 0px; width: 1903px;">
        <div class="uk-container uk-container-expand">
            <nav class="uk-navbar">
                <div class="uk-navbar-left">
                    <a href="../" class="uk-navbar-item uk-logo">
                        <img uk-svg="" src="../images/uikit-logo.svg" class="uk-margin-small-right" hidden="true"><svg width="28" height="34" viewBox="0 0 28 34" xmlns="http://www.w3.org/2000/svg" class="uk-margin-small-right uk-svg" data-svg="../images/uikit-logo.svg">
                            <polygon fill="#fff" points="19.1,4.1 13.75,1 8.17,4.45 13.6,7.44 "></polygon>
                            <path fill="#fff" d="M21.67,5.43l-5.53,3.34l6.26,3.63v9.52l-8.44,4.76L5.6,21.93v-7.38L0,11.7v13.51l13.75,8.08L28,25.21V9.07 L21.67,5.43z"></path>
                        </svg> UIkit
                    </a>
                </div>
                <div class="uk-navbar-right">
                    <ul class="uk-navbar-nav uk-visible@m">
                        <li><a href="../pro">Pro</a></li>
                        <li class="uk-active"><a href="./">Documentation</a></li>
                        <li><a href="../changelog">Changelog</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>

    <div class="uk-container">
        @RenderBody()

        <div class="uk-text-center uk-margin-xlarge-top">&copy; 2020 - CoreForm</div>
    </div>


    <script type="text/javascript" src="@Url.Content("~/lib/jquery/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/lib/uikit/js/uikit.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/lib/uikit/js/uikit-icons.min.js")"></script>


    @RenderSection("Scripts", required: false)
    @RenderSection("LocalScripts", required: false)
</body>
</html>
